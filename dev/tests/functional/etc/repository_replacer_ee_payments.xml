<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Cybersource\Test\Repository\SandboxCustomer">
        <dataset name="sandbox_default">
            <field name="merchant_id" xsi:type="string">CYBERSOURCE_SANDBOX_MERCHANT_ID</field>
            <field name="username" xsi:type="string">CYBERSOURCE_SANDBOX_USERNAME</field>
            <field name="password" xsi:type="string">CYBERSOURCE_SANDBOX_PASSWORD</field>
        </dataset>
    </repository>
    <repository class="Magento\Signifyd\Test\Repository\SignifydAccount">
        <dataset name="sandbox_default">
            <field name="email" xsi:type="string">SIGNIFYD_EMAIL</field>
            <field name="password" xsi:type="string">SIGNIFYD_PASSWORD</field>
        </dataset>
    </repository>
</config>
